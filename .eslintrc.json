{
  "extends": ["eslint:recommended", "prettier", "react-app", "react-app/jest", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": ["error", { "properties": "never" }],
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "eqeqeq": ["error", "always"],
    "no-unused-vars": ["error"]
  }
}
